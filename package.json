{
  "name": "tg-parser",
  "private": true,
  "publishConfig": {
    "access": "restricted"
  },
  "version": "0.0.0",
  "description": "Telegram Channel Parser (Console Mode)",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "prettier --ignore-path ./.lintignore . '!data' --check && eslint . --ext .ts --ignore-path ./.lintignore",
    "lint:fix": "prettier --ignore-path ./.lintignore --write . '!data' --check && eslint . --ext .ts --ignore-path ./.lintignore --fix",
    "build": "tsup",
    "start": "tsx src/index.ts",
    "start:dev": "node --experimental-specifier-resolution=node --loader ts-node/esm src/index.ts",
    "api": "NODE_ENV=api tsx src/index.ts"
  },
  "devDependencies": {
    "@types/node": "^20.17.57",
    "@types/debug": "^4.1.12",
    "typescript": "^5.8.3",
    "tsup": "^8.5.0",
    "ts-node": "^10.9.2",
    "prettier": "^3.5.3",
    "eslint": "^8.57.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.4.1"
  },
  "dependencies": {
    "debug": "^4.4.1",
    "dotenv": "16.4.5",
    "express": "^4.21.2",
    "telegram": "^2.26.22",
    "tsx": "^4.19.4"
  }
}
